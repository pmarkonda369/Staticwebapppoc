{
    "routes": [
        {
            "route": "/authenticated/*",
            "allowedRoles": [ "authenticated" ]
        },
        {
            "route": "/admin/*",
            "allowedRoles": [ "admin" ]
        }
    ],
    "auth": {
        "identityProviders": {
            "azureActiveDirectory": {
                "userDetailsClaim": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
                "registration": {
                    "openIdIssuer": "https://login.microsoftonline.com/45165946-d5b0-4c17-886f-178ab1f93ed1",
                    "clientIdSettingName": "<your_client_id_setting_name>",
                    "clientSecretSettingName": "<your_client_secret_setting_name>"
                }
            }
        },
        "routes": {
            "^/admin/.*$": {
                "allowedRoles": [ "admin" ]
            }
        }
    },
    "globalHeaders": {
        "Cache-Control": "no-cache"
    }
}
